{% block htmlWrapper %}
    <!DOCTYPE html>
    <html lang="{{ app.request.getLocale() }}">
    {% block html %}

        {% block headWrapper %}
            <head>
                {% block metaWrapper %}
                    <title>{% block title %}{% endblock %}</title>
                    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
                    <meta name="viewport" content="width=device-width, initial-scale=1.0">
                {% endblock %}

                {% block stylesheets %}
                    {% include 'ESCoreBundle:Partials:stylesheets.html.twig' %}
                {% endblock %}
            </head>
        {% endblock %}

        {% block bodyWrapper %}
            <body class="eupopup eupopup-bottom">
            {% block body %}
                <div {% block contentAttr -%}id="a"{%- endblock %}>
                    {% block header %}
                        {% include 'ESCoreBundle:Partials:header.html.twig' %}
                    {% endblock %}

                    {% block flashes %}
                        <div class="container">
                            <div class="row">
                                <div class="col-md-12">
                                    {% include 'ESCoreBundle:Partials:FlashMessage.html.twig' %}
                                </div>
                            </div>
                        </div>
                    {% endblock %}

                    {% block container %}
                        <div class="container">
                            <div class="row">
                                <div class="col-md-12">
                                    {% block content %}{% endblock %}
                                </div>
                            </div>
                        </div>
                    {% endblock %}

                    {% block footer %}
                        {% include 'ESCoreBundle:Partials:footer.html.twig' %}
                    {% endblock %}

                    {% block cookies %}
                        add cokie script
                    {% endblock %}
                </div>

                {% block javascripts %}
                    {% include 'ESCoreBundle:Partials:javascripts.html.twig' %}
                {% endblock %}

            {% endblock %}
            </body>
        {% endblock %}

    {% endblock %}
    </html>
{% endblock %}